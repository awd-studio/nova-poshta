{
    "name": "awd-studio/nova-poshta",
    "description": "PHP SDK for Nova Poshta API",
    "keywords": [
        "delivery",
        "delivery-api",
        "nova",
        "novaposhta",
        "novaposhta-api",
        "poshta",
        "post",
        "postservices",
        "sdk",
        "ukraine",
        "wrapper-api"
    ],
    "homepage": "https://github.com/awd-studio/nova-poshta",
    "type": "library",
    "require": {
        "php": ">=5.5 | 7"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "php-coveralls/php-coveralls": "^2.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Karpov",
            "email": "awd.com.ua@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/awd-studio/nova-poshta/issues",
        "source": "https://github.com/awd-studio/nova-poshta"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "NovaPoshta\\": "src/NovaPoshta/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NovaPoshta\\Tests\\": "tests/NovaPoshta/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
